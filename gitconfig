[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
[user]
	name = Shan Gardezi
	email = shangardezi@gmail.com
[core]
	excludesfile = /Users/shan/.gitignore_global
	editor = nvim
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[filter "strongbox"]
	clean = strongbox -clean %f
	smudge = strongbox -smudge %f
	required = true
[diff "strongbox"]
	textconv = strongbox -diff
[url "git@github.com:"]
	insteadOf = https://github.com/
	insteadOf = https://github.com/
	insteadOf = https://github.com/
	insteadOf = https://github.com/
[url "https://"]
	insteadOf = git://
